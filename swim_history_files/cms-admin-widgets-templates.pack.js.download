window.__TEMPLATE_BUNDLE["/cms/admin/widgets/web-design/EditContactInfoDialog.xhtml"] = "<vbox xmlns:ui=\"http:\/\/evolus.vn\/Namespaces\/WebUI\/1.0\">\n    \n    <ui:HintBox>\n        Fill out your organization\u2019s contact information and business hours to be displayed on the page.\n    Each section can be toggled on or off. Click the clock icon in the Hours section to toggle\n    between a time display or a text display.\n    <\/ui:HintBox>\n    <hbox class=\"Location Section Border\">\n        <vbox class=\"LocationBox\">\n            <label class=\"Title\" anon-id=\"locationNameLable\" for=\"locationNameInput\">Location Name:<\/label>\n            <input type=\"text\" value=\"Location\" anon-id=\"locationNameInput\" placeholder=\"Input Location Name\"\/>\n        <\/vbox>\n        <vbox class=\"CopyInfoBox\">\n            <div class=\"Title\">1<\/div>\n            <button type=\"button\" anon-id=\"copyInfoButton\" >Copy info from Org Profile<\/button>\n        <\/vbox>\n    <\/hbox>\n    <div class=\"Section Border\">\n        <hbox class=\"Title\">\n            <input type=\"text\" value=\"Address\" anon-id=\"addressTitle\" placeholder=\"Input Address Title\"\/>\n        <\/hbox>\n        <vbox class=\"Body\">\n            <div class=\"Block\">\n                <div class=\"Entry\">\n                    <label for=\"addressInput\">Address:<\/label>\n                    <input type=\"text\" anon-id=\"addressInput\" \/>\n                <\/div>\n                <div class=\"Entry\">\n                    <label for=\"address2Input\">Address 2:<\/label>\n                    <input type=\"text\" anon-id=\"address2Input\" \/>\n                <\/div>\n            <\/div>\n            <div class=\"Block\">\n                <div class=\"Entry\">\n                    <label for=\"cityInput\">City:<\/label>\n                    <input type=\"text\" anon-id=\"cityInput\" \/>\n                <\/div>\n                <div class=\"Entry\">\n                    <label for