    var rs = new RuleSet().live(true);
    var answerViewWidgets = this._collectQuestionAnswerViewWidgets();
    answerViewWidgets.forEach((w, i) => {
        var set = w.getValidationRuleSet();
        if (set) rs.set(set);
    });
    return rs;
